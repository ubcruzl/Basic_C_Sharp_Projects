<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application- Ensuree</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    @Html.ActionLink("Insuree", "../Insuree/Index", "Home", new { area = "" }, new { @class = "nav-itemnavbar-light bg-light" })
    @Html.ActionLink("About", "About", "Home", null, new { @class = "nav-item navbar-light bg-light" })
    @Html.ActionLink("Contact", "Contact", "Home", null, new { @class = "nav-item navbar-light bg-light" })
    @Html.ActionLink("Admin", "../Admin/Index", "Home", null, new { @class = "nav-item navbar-light bg-light" })
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application- Ensuree</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
